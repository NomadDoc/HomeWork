import random
with open("texts.txt", "w") as write_file:
    for i in range(10):
        write_file.write(str(random.randint(1, 101)) + " ")
with open("texts.txt") as read_file:
    read_line = read_file.readline()
    print(" " + read_line)
    res_sum = 0
    for str_number in read_line.split():
        res_sum += int(str_number)
    print(f"Сумма: {res_sum}")
